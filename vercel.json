{
  "version": 2,
  "builds": [
    {
      "src": "build/web",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    },
    {
      "src": "/(.+\\.[a-z0-9]+)$",
      "dest": "/build/web/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/build/web/index.html"
    }
  ]
}

