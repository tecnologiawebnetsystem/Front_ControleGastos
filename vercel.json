{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build/web",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.[a-z0-9]+)$",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}

